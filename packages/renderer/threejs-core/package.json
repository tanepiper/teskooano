{
  "name": "@teskooano/renderer-threejs-core",
  "version": "0.2.0",
  "private": true,
  "description": "Core components for the ThreeJS renderer (Scene, Animation, State)",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:browser": "vitest --browser=chromium",
    "test:watch": "vitest",
    "test:browser:watch": "vitest --browser=chromium",
    "test:browser:headless": "vitest --browser.headless",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@teskooano/core-state": "file:../../core/state",
    "@teskooano/data-types": "file:../../data/types",
    "three": "0.176.0"
  },
  "devDependencies": {
    "@types/three": "0.176.0",
    "@playwright/test": "1.52.0",
    "@vitest/browser": "3.1.2",
    "@vitest/ui": "3.1.2",
    "eslint": "9.25.1",
    "happy-dom": "17.4.6",
    "playwright": "1.52.0",
    "typescript": "5.8.3",
    "vitest": "3.1.2"
  }
}
